struct Foo {
  field1: string;
  field2: string;
}

fun Foo.foo(a: string, b: string) {
  this.field1 = a;
  this.field2 = b;
}

fun Foo.fooPrint() {
  print(this.field1);
  print(this.field2);
}

struct Bar : Foo {}

fun Bar.bar(a: string, b: string) {
  this.field1 = a;
  this.field2 = b;
}

fun Bar.barPrint() {
  print(this.field1);
  print(this.field2);
}

var bar: Bar;
bar.foo("foo 1", "foo 2");
bar.fooPrint();
// expect: foo 1
// expect: foo 2

bar.bar("bar 1", "bar 2");
bar.barPrint();
// expect: bar 1
// expect: bar 2

bar.fooPrint();
// expect: bar 1
// expect: bar 2
