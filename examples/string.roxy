native fun print(value: string);
native fun concat(a: string, b: string);

fun concat3(a: string, b: string, c: string): string {
    return a + b + c;
}

var hello = "Hello";
var world = "world";

var hello_world = hello + " " + world + "!";
print(hello_world);
print(concat3("Hello world", " again", "!"));